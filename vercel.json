{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/dist/public/icons/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/dist/public/manifest.json"
    },
    {
      "src": "/service-worker.js",
      "dest": "/dist/public/service-worker.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
